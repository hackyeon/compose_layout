<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hackyeon.compose_layout">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Compose_layout"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".BuiltInActivity"
            android:exported="true"
            android:label="@string/title_activity_built_in"
            android:theme="@style/Theme.Compose_layout" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ConstraintActivity"
            android:exported="true"
            android:label="@string/title_activity_constraint"
            android:theme="@style/Theme.Compose_layout">

        </activity>
        <activity
            android:name=".GridActivity"
            android:exported="true"
            android:label="@string/title_activity_grid"
            android:theme="@style/Theme.Compose_layout"></activity>
        <activity
            android:name=".LayoutActivity"
            android:exported="false"
            android:label="@string/title_activity_layout"
            android:theme="@style/Theme.Compose_layout" />
        <activity
            android:name=".ListActivity"
            android:exported="true"
            android:label="@string/title_activity_list_string"
            android:theme="@style/Theme.Compose_layout" />
        <activity
            android:name=".ScaffoldActivity"
            android:exported="true"
            android:label="@string/title_activity_scaffold"
            android:theme="@style/Theme.Compose_layout" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Compose_layout" />
    </application>

</manifest>